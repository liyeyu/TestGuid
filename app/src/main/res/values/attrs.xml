<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="parallax_attr">
        <attr name="a_in" format="float"></attr>
        <attr name="a_out" format="float"></attr>
        <attr name="x_in" format="float"></attr>
        <attr name="x_out" format="float"></attr>
        <attr name="y_in" format="float"></attr>
        <attr name="y_out" format="float"></attr>
    </declare-styleable>

    <declare-styleable name="rounded_image_view">
        <attr name="border_thickness" format="dimension"/>
        <attr name="border_inside_color" format="color"/>
        <attr name="border_outside_color" format="color"></attr>
    </declare-styleable>
</resources>
